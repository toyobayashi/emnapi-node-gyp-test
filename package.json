{
  "name": "emnapi-node-gyp-test",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "postinstall": "node ./scripts/patch.js",
    "build-emscripten:win": "build.bat -f make-emscripten",
    "build-emscripten-threads:win": "build.bat -f make-emscripten -Dwasm_threads=1",
    "build-wasi:win": "build-wasi.bat -f make-wasi",
    "build-wasi-threads:win": "build-wasi.bat -f make-wasi -Dwasm_threads=1",
    "build-wasm32:win": "build-wasi.bat -f make-wasm",
    "build-wasm32-threads:win": "build-wasi.bat -f make-wasm -Dwasm_threads=1",
    "build-emscripten:unix": "./build.sh -f make-emscripten",
    "build-emscripten-threads:unix": "./build.sh -f make-emscripten -Dwasm_threads=1",
    "build-wasi:unix": "./build-wasi.sh -f make-wasi",
    "build-wasi-threads:unix": "./build-wasi.sh -f make-wasi -Dwasm_threads=1",
    "build-wasm32:unix": "./build-wasi.sh -f make-wasm",
    "build-wasm32-threads:unix": "./build-wasi.sh -f make-wasm -Dwasm_threads=1",
    "start": "node ./main.js"
  },
  "dependencies": {
    "@emnapi/core": "1.1.0",
    "@emnapi/runtime": "1.1.0",
    "emnapi": "1.1.0",
    "node-addon-api": "*"
  },
  "devDependencies": {
    "node-gyp": "10.0.1"
  }
}
